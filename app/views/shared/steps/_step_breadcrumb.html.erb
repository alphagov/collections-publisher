<% content_for :breadcrumbs do %>
  <% if links.any? %>
    <%= render "govuk_publishing_components/components/breadcrumbs", {
      breadcrumbs: links.map do |link| 
        {
          :title => link[:text],
          :url => link[:href].present? ? url_for(link[:href]) : nil
        }
      end
    } %>
  <% end %>
<% end %>

<%= render 'coronavirus/shared/breadcrumbs', text: t("coronavirus.timeline_entries.new.title")  %>
<% content_for :title, t("coronavirus.shared.title") %>
<% content_for :context, t("coronavirus.timeline_entries.new.title") %>
<% if @timeline_entry.errors.any? %>
  <%= render "shared/sub_sections/form_errors", resource: @timeline_entry %>
<% end %>

<div class="covid-edit-page govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <%= form_with url: coronavirus_page_timeline_entries_path do %>
      <%= render "form" %>
    <% end %>
  </div>
</div>
